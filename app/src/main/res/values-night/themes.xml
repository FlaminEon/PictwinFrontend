        <item name="colorPrimary">@color/primary_amber_dark</item>
        <item name="colorPrimaryVariant">@color/primary_amber_600</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/secondary_lightblue_dark</item>
        <item name="colorSecondaryVariant">@color/secondary_lightblue_700</item>
